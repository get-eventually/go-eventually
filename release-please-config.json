{
  "separate-pull-requests": false,
  "sequential-calls": true,
  "bump-minor-pre-major": true,
  "bump-patch-for-minor-pre-major": true,
  "tag-separator": "/",
  "packages": {
    "core": {
      "release-type": "go",
      "package-name": "core"
    },
    "firestore": {
      "release-type": "go",
      "package-name": "firestore"
    },
    "oteleventually": {
      "release-type": "go",
      "package-name": "oteleventually"
    },
    "postgres": {
      "release-type": "go",
      "package-name": "postgres"
    },
    "serdes": {
      "release-type": "go",
      "package-name": "serdes"
    },
    "integrationtest": {
      "release-type": "go",
      "package-name": "integrationtest"
    }
  },
  "plugins": [
    {
      "type": "linked-versions",
      "groupName": "firestore->core",
      "components": ["firestore", "core"]
    },
    {
      "type": "linked-versions",
      "groupName": "oteleventually->core",
      "components": ["oteleventually", "core"]
    },
    {
      "type": "linked-versions",
      "groupName": "postgres->core",
      "components": ["postgres", "core", "integrationtest"]
    },
    {
      "type": "linked-versions",
      "groupName": "serdes->core",
      "components": ["serdes", "core"]
    }
  ],
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}
